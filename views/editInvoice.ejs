<html>
    <head>
        <title>Invoice Generator</title>
    </head>
    <body>

        <h3>EDIT INVIOCE</h3>

        <form method="POST" action="/myAccount/invoices">
            <label>title</label>
            <input type="text" name="title" value=<%= invoiceToEdit.title %>>
            <br>
            <label>Date</label>
            <input type="text" name="date" value=<%= invoiceToEdit.date %>>
            <br>
            <label>Bill from</label>
            <input type="text" name="billFrom" value=<%= invoiceToEdit.billFrom %>>
            <br>
            <label>Bill to</label>
            <input type="text" name="billTo" value=<%= invoiceToEdit.billTo %>>
            <br><br>
            <input type="submit" value="Save changes">
        </form>

    </body>
</html>